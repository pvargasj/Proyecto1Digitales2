`include "phy_tx_cond.v"
//`include "phy_rx_cond.v"
`include "clkgen.v"

module PCIe_cond(input [7:0] data_in_0_c,
                 input [7:0] data_in_1_c,
                 input valid_in_0_c,
                 input valid_in_1_c,
                 input clk_8f,
                 input resetCLK,
                 input reset);
                 //output reg [7:0] data_out_c_0,
	           //  output reg [7:0] data_out_c_1,
                 //output reg valid_out_c_0,
                 //output reg valid_out_c_1)

/*AUTOWIRE*/
// Beginning of automatic wires (for undeclared instantiated-module outputs)
wire			clk2f_c;		// From Relojes of clkgen.v
wire			clkf_c;			// From Relojes of clkgen.v
// End of automatics


clkgen      Relojes(/*AUTOINST*/
		    // Outputs
		    .clkf_c		(clkf_c),
		    .clk2f_c		(clk2f_c),
		    // Inputs
		    .clk_8f		(clk_8f),
		    .resetCLK		(resetCLK));

phy_tx_cond TX (/*AUTOINST*/);

endmodule

